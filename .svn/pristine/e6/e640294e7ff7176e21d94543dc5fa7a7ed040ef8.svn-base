.submit-container {
   margin:3px 0;
   text-align:right;
   }
.submit-button {
    width: 100px;
    height: 30px;
   border: 1px solid #8c8c8c;
   background: #858585;
   background: -webkit-gradient(linear, left top, left bottom, from(#ffffff), to(#858585));
   background: -webkit-linear-gradient(top, #ffffff, #858585);
   background: -moz-linear-gradient(top, #ffffff, #858585);
   background: -ms-linear-gradient(top, #ffffff, #858585);
   background: -o-linear-gradient(top, #ffffff, #858585);
   background-image: -ms-linear-gradient(top, #ffffff 0%, #858585 100%);
   -webkit-border-radius: 8px;
   -moz-border-radius: 8px;
   border-radius: 8px;
   -webkit-box-shadow: rgba(000,000,000,0.5) 1px 1px 1px, inset rgba(000,000,000,0.5) 0 0 0;
   -moz-box-shadow: rgba(000,000,000,0.5) 1px 1px 1px, inset rgba(000,000,000,0.5) 0 0 0;
   box-shadow: rgba(000,000,000,0.5) 1px 1px 1px, inset rgba(000,000,000,0.5) 0 0 0;
   text-shadow: #bfe4eb 0 1px 0;
   color: #c44848;
   font-family: helvetica, serif;
   padding: 3px 5px;
   font-size: 14px;
   text-shadow: #000000 0 1px 0;
   text-decoration: none;
   vertical-align: middle;
   }